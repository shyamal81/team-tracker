{{#partial "content"}}

    <p>Member Details:</p>

    <p>{{ definition.description }}</p>
    <h5>Created At:</h5>
    <p> {{ definition.createdAt }}</p>
    <h5>word status:</h5>
    {{#if definition.published }}
        <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
    {{else}}
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
    {{/if}}

    <a href="/definitions/{{definition.id}}/update">Edit this Member name</a><br>

    </p>

{{/partial}}

{{> layout.hbs}}