{{#partial "content"}}

    <h1>Registration Confirmed. New member has been added to team IT {{descriptions}}!</h1>

    {{#if descriptions}}
        {{#each descriptions }} <!--loop-->
        <p>{{ descriptions }}  <a href="/definitions/{{id}}"> </a> </p> <!--show-->
        <p>{{ createdAt }}</p> <!--show-->
        {{/each}}
    {{else}}
        <p>Name not entered!</p>

    {{/if}}

    <a href="/">Go Back</a>
{{/partial}}

{{> layout.hbs}}